{
	"sap.app": {
		"id": "sap.m.sample.sampleapp4",
		"i18n": "i18n/i18n.properties",
		"dataSources": {
			"modelalias": {
             "uri": "https://services.odata.org/v2/northwind/northwind.svc/" , 
			 "type": "OData" 
         }
		},
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
  	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.m.sample.sampleapp4.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
    "models": {
            "mymodel": {
                "preload": true,
                "dataSource": "modelalias"
            },
			"i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.m.sample.sampleapp4.i18n.i18n"
                }
            }
        },
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "sap.m.sample.sampleapp4.view",
				"controlId": "rootControl",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
			{
				"pattern": "",
				"name": "main",
				"target": "main"
			},
			
			{
				"pattern": "qr",
				"name": "qr",
				"target": "qr"
			},
			{
				"pattern": "todo",
				"name": "todo",
				"target": "todo"
			},
			{
				"pattern": "expensemgmt",
				"name": "expensemgmt",
				"target": "expensemgmt"
			},
			{
				"pattern": "table",
				"name": "table",
				"target": "table"
			},
			{
				"pattern": "cardlayout",
				"name": "cardlayout",
				"target": "cardlayout"
			},
			{
				"pattern": "masterdetail",
				"name": "masterdetail",
				"target": "masterdetail"
			},
			{
				"pattern": "pdf",
				"name": "pdf",
				"target": "pdf"
			},
			{
				"pattern": "worklist",
				"name": "worklist",
				"target": "worklist"
			},
			{
				"pattern": "Orders/{objectId}",
				"name": "object",
				"target": [
					"object"
				]
            }
			],
			"targets": {
				"main": {
					"id": "View0",
					"name": "View0",
					"level" : 0
				},
				"qr": {
					"id": "View6",
					"name": "View6",
					"level" : 1
				},
				"todo": {
					"id": "View1",
					"name": "View1",
					"level" : 1
				},
				"table": {
					"id": "View2",
					"name": "View2",
					"level" : 1
				},
				"cardlayout": {
					"id": "View3",
					"name": "View3",
					"level" : 1
				},
				"masterdetail": {
					"id": "View4",
					"name": "View4",
					"level" : 1
				},
				"pdf": {
					"id": "View5",
					"name": "View5",
					"level" : 1
				},
				"expensemgmt": {
					"id": "View7",
					"name": "View7",
					"level" : 1
				},
				"worklist": {
					"id": "View8",
					"name": "view8",
					"level" : 1
				},
				"object": {
                    "viewName": "Object",
                    "viewId": "object",
                    "viewLevel": 2,
                    "title": "{i18n>objectViewTitle}"
                }			
			}
		}
	}
}
